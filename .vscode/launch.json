{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Ruff",
            "type": "debugpy",
            "request": "launch",
            "module": "ruff",
            "cwd": "${workspaceFolder}",
            "args": [
                "check",
                "--config",
                "pyproject.toml",
                "--fix",
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "Mypy",
            "type": "debugpy",
            "request": "launch",
            "module": "mypy",
            "cwd": "${workspaceFolder}",
            "args": [
                "--config-file",
                "pyproject.toml",
                "src",
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "justMyCode": false,
        },
        {
            "name": "Octoprobe main",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.main",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op install",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "install",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op commissioning",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "commissioning",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op bootmode-infra",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "bootmode-infra",
                "--picotool-cmd",
                "--poweron",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op bootmode-probe",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "bootmode-probe",
                "--picotool-cmd",
                "--poweron",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op flash-infra",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "flash-infra",
                "${userHome}/octoprobe_downloads/cache_firmware/RPI_PICO-20240602-v1.23.0.uf2",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op flash-probe",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "flash-probe",
                "${userHome}/Downloads/debugprobe_on_pico.uf2",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op cycle dut",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "powercycle",
                "dut",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op cycle infra --serial xxx",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "powercycle",
                "infra",
                // "--serial",
                // "d32",
                // "e4636874db405d32",
                // "--serial",
                // "e4636874db124f35",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op power",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "power",
                "--off",
                "infraboot",
                "--off",
                "dut",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op query",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "query",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op query --poweron",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "query",
                "--poweron",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op exec-infra",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "exec-infra",
                "--serials=5425",
                "set_relays([(5, True)])",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "op udev",
            "type": "debugpy",
            "request": "launch",
            "module": "octoprobe.scripts.op",
            "cwd": "${workspaceFolder}",
            "args": [
                "udev",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceFolder}",
            "args": [
                // "--collect-only",
                // "-q",
                "-s",
                "-v",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            },
            "justMyCode": false,
        },
    ]
}